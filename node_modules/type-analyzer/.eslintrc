{
  "extends": [
    "eslint-config-prettier",
    "eslint-config-uber-es2015",
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "prettier",
    "es5"
  ],
  "rules": {
    "valid-jsdoc": 0,
    "no-var": 0,
    "max-len": 0,
    "quote-props": 1,
    "spaced-comment": 1,
    "max-params": 0,
    "prefer-template": 0,
    "no-inline-comments": 0,
    "no-invalid-this": 0,
    "no-unused-expressions": 0,
    "camelcase": 0,
    "consistent-return": 0,
    "comma-dangle": 1,
    "strict": 0,
    "object-shorthand": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
